@using BS.Model;
@model List<Category>

<div class="col_1_of_4 span_1_of_4">
    <h4>Chủ đề</h4>
    <ul>
        @foreach (var item in Model)
        {
            <li><a href="@Url.Action("BookBaseOnCategory","Category",new { @categoryId = item.CategoryId})">@item.Name</a></li>
        }
        
    </ul>
</div>